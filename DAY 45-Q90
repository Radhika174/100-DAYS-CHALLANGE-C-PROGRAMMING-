/*Q90: Toggle case of each character in a string.

Sample Test Cases:
Input 1:
Hello
Output 1:
hELLO

*/
#include <stdio.h>
#include <string.h>
int main() 
{
    char strg[100];
    printf("Enter string");
    scanf("%[^\n]",strg);
    char ch;
    char news[100];
    for(int i=0;i<strlen(strg);i++)
    {
      ch=strg[i];
      if(ch>='A' && ch<='Z')
      ch=ch+32; 
      else
      ch=ch-32;
      news[i]=ch;
    }
    printf("%s",news);
    
  
    return 0;
}
