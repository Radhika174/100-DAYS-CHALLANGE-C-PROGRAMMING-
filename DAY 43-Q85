/*Q85: Reverse a string.

Sample Test Cases:
Input 1:
abcd
Output 1:
dcba

*/
#include <stdio.h>
#include <string.h>
int main() 
{
    char strg[100];
    printf("Enter string");
    scanf("%[^\n]",strg);
    char ch;
    char news[100];
    int p=0;
    for(int i=strlen(strg)-1;i>=0;i--)
    {
      ch=strg[i];
      news[p]=ch;
      p++;
    }
    printf("%s",news);
    
  
    return 0;
}
