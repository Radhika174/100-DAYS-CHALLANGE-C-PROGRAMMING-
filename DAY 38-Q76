/*Q76: Check if a matrix is symmetric.

Sample Test Cases:
Input 1:
2 2
1 2
2 1
Output 1:
True

Input 2:
2 2
1 0
2 1
Output 2:
False

*/

#include <stdio.h>

int main()
{
    int r1,c1;
    printf("Enter the row and column size of  array");
    scanf("%d%d",&r1,&c1);
    int a[r1][c1];
    int b[r1][c1];
    
    for(int i=0;i<r1;++i)
    {
        for(int j=0;j<c1;++j)
        {
            printf("Enter a num");
            scanf("%d",&a[i][j]);
        }
    }
    int c=0;
    for(int i=0;i<c1;++i)
    {
        for(int j=0;j<r1;++j)
        {
           if((a[i][j]==a[j][i]))
           ++c;
        }
    }
  
    if(c==r1*c1)
    printf("Symmetric");
    else
    printf("not Symmetric");
     
}
