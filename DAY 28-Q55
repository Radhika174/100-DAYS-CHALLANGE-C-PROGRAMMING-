/*Q55: Write a program to print all the prime numbers from 1 to n.

Sample Test Cases:
Input 1:
10
Output 1:
2 3 5 7

Input 2:
20
Output 2:
2 3 5 7 11 13 17 19

*/
#include <stdio.h>
int main()
{
    int n;
    printf("Enter number of elements in the array:");
    scanf("%d",&n);
    int a[n];
    printf("\nEnter array elements:");
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int c;
    for(int i=0;i<n;i++)
    {
        c=0;
       for(int j=1;j<=a[i];j++)
       {
           if(a[i]%j==0)
           c++;
       }
       if(c==2)
       printf("%d",a[i]);
       printf("\n");
    }

return 0;
}
